vowel={'a':0,
       'e':0,
       'i':0,
       'o':0,
       'u':0,}
Word=str(input("Enter a word: "))
Word=list(Word.lower())
i=0
ks=vowel.keys() 
for i in range(len(Word)):
    print(Word[i])
    if Word[i] in ks:
            print("in", Word[i])
            vowel[Word[i]]=vowel[Word[i]]+1    
    i=i+1
print("A=",vowel['a'],"E=",vowel['e'],"I=",vowel['i'],"O=",vowel['o'],"U=",vowel['u'])

